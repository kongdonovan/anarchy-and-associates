{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "entryPointStrategy": "expand",
  "out": "./docs/generated",
  "exclude": [
    "**/__tests__/**/*",
    "**/test/**/*",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/scripts/**/*",
    "**/dev-server.ts",
    "**/simple-dev.ts",
    "**/node_modules/**/*"
  ],
  "excludeExternals": false,
  "includeVersion": true,
  "name": "Anarchy & Associates Discord Bot",
  "readme": "./CLAUDE.md",
  "theme": "default",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-merge-modules"
  ],
  "githubPages": false,
  "hideGenerator": false,
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Domain",
    "Application",
    "Infrastructure",
    "Presentation",
    "*"
  ],
  "groupOrder": [
    "Domain",
    "Application Services",
    "Infrastructure",
    "Presentation",
    "*"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": false
  },
  "searchInComments": true,
  "searchInDocuments": true,
  "skipErrorChecking": false,
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  },
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": false
  },
  "sort": ["source-order"],
  "kindSortOrder": [
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Function",
    "Variable",
    "Constructor",
    "Property",
    "Method",
    "Reference"
  ],
  "preserveLinkText": true,
  "blockTags": [
    "@alpha",
    "@beta",
    "@deprecated",
    "@example",
    "@experimental",
    "@hidden",
    "@ignore",
    "@internal",
    "@override",
    "@readonly",
    "@sealed",
    "@virtual",
    "@group",
    "@category",
    "@module"
  ],
  "modifierTags": [
    "@public",
    "@private",
    "@protected",
    "@readonly",
    "@override"
  ],
  "commentStyle": "all",
  "useTsLinkResolution": true,
  "preserveWatchOutput": false,
  "cleanOutputDir": true,
  "titleLink": "/",
  "navigationLinks": {
    "GitHub": "https://github.com/anarchy-associates/discord-bot",
    "README": "./README.md",
    "Architecture": "./CLAUDE.md"
  },
  "sidebarLinks": {
    "Project Documentation": "./CLAUDE.md",
    "Command Overview": "./COMMAND_OVERVIEW.md",
    "Configuration": "./CONFIGURATION.md"
  },
  "requiredToBeDocumented": [
    "Enum",
    "Class",
    "Interface",
    "Function"
  ],
  "lightHighlightTheme": "github-light",
  "darkHighlightTheme": "github-dark",
  "outputFileStrategy": "members",
  "parametersFormat": "table",
  "typeDeclarationFormat": "table",
  "tableColumnSettings": {
    "hideDefaults": false,
    "hideInherited": false,
    "hideModifiers": false,
    "hideOverrides": false,
    "hideSources": false,
    "hideValues": false
  },
  "customCss": "./docs/custom.css",
  "favicon": "./docs/favicon.ico"
}